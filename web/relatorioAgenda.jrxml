	<parameter name="codUsuario" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
     pessoa.`cod_pessoa` AS pessoa_cod_pessoa,
     pessoa.`nome` AS pessoa_nome,
     pessoa.`endereco` AS pessoa_endereco,
     pessoa.`telefone1` AS pessoa_telefone1,
     pessoa.`telefone2` AS pessoa_telefone2,
     cidade.`nome` AS cidade_nome,
     estado.`sigla_estado` AS sigla_estado
FROM
     `estado` estado INNER JOIN `cidade` cidade ON estado.`sigla_estado` = cidade.`sigla_estado`
     INNER JOIN `pessoa` pessoa ON cidade.`cod_cidade` = pessoa.`cod_cidade`
WHERE pessoa.cod_usuario = $P{codUsuario}]]>
